{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
        "cwd": "${workspaceRoot}",
        "executable": "${workspaceRoot}/cmake_build/DISCO_F412ZG/develop/GCC_ARM/${workspaceFolderBasename}.elf",
        "name": "Debug STM",
        "request": "launch",
        "type": "cortex-debug",
        "servertype": "jlink",//The choice of debugging emulator, which can support opennocd, pyocd, pe and stutil
        "runToMain": true, //Stop at the main function as soon as debugging
        "device": "STM32F412ZG",//Model of the device
        "interface": "swd",//SWD interface
        "showDevDebugOutput": false,
        "svdFile": "${workspaceRoot}/.vscode/STM32F412.svd",
        "hardwareBreakpoints": {
        "require": true,
        "limit": 4
      },
        "configFiles": [
                "interface/stlink-v2-1.cfg",
                "target/stm32f4x.cfg"
            ],
         "swoConfig": {
                "enabled": true,
                "cpuFrequency": 8000000,
                "swoFrequency": 4000000,
                "source": "probe",
                "decoders": [
                    { "type": "console", "label": "ITM", "port": 0 }
                ]
            }
        },
    ]
}
